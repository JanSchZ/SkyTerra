# Example .env file for SkyTerra project
# Copy this file to .env and fill in your actual values

# --- Django Backend Variables ---
DEBUG=True
SECRET_KEY=your_django_secret_key
DATABASE_URL=postgres://user:password@host:port/database_name

# API Keys and Secrets
GEMINI_API_KEY=your_gemini_api_key
AWS_STORAGE_BUCKET_NAME=your_s3_bucket_name
# Add other backend-specific variables here (e.g., for email, other services)


# --- React Frontend Variables ---
# VITE variables must be prefixed with VITE_
# Make sure to use your *public* Mapbox token here
VITE_MAPBOX_TOKEN=YOUR_MAPBOX_PUBLIC_ACCESS_TOKEN
VITE_API_BASE_URL=http://localhost:8000/api
# Add other frontend-specific variables here
