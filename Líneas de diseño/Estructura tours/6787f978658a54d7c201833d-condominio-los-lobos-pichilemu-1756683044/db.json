lemu","path":"\/uploads\/vr\/6787f978658a54d7c201833d\/asset\/Logo_los_lobos_pichilemu-1740009798-0.avif","thumb":"\/uploads\/vr\/6787f978658a54d7c201833d\/asset\/Logo_los_lobos_pichilemu-1740009798-0.avif","width":150,"height":null,"type":"asset","mime_type":"image\/avif","project_id":"6787f978658a54d7c201833d","bucket_version":2,"updated_at":"2025-02-20 07:03:18","created_at":"2025-02-20 07:03:18","oldWidth":null,"oldHeight":null,"scenes":{"6787f9883db7ae78650d526c":true},"margin_top":4,"margin_bottom":10,"margin_left":4,"margin_right":10,"align":"top-left","link":"www.loslobospichilemu.cl"}},"version":1}},{"slug":"customcode","name":"Custom code","config":{"custom_html":"<!-- Esto no cambiar\u00e1 nada visualmente, pero asegura que los hotspots tengan la clase correcta -->\r\n<script>\r\ndocument.addEventListener(\"DOMContentLoaded\", function () {\r\n    const hotspots = document.querySelectorAll(\".lapentor-hotspot img\");\r\n    hotspots.forEach(hotspot => {\r\n        hotspot.classList.add(\"custom-hotspot\");\r\n    });\r\n});\r\n<\/script>\r\n\r\n<script>\r\ndocument.addEventListener(\"DOMContentLoaded\", function () {\r\n    function updateHotspotSize() {\r\n        document.querySelectorAll(\".custom-hotspot\").forEach(hotspot => {\r\n            let distance = calculateDistance(hotspot);\r\n            let scale = Math.max(0.5, Math.min(1.5, 3 \/ distance)); \r\n            hotspot.style.transform = `scale(${scale})`;\r\n        });\r\n    }\r\n\r\n    function calculateDistance(hotspot) {\r\n        let rect = hotspot.getBoundingClientRect();\r\n        let centerX = window.innerWidth \/ 2;\r\n        let centerY = window.innerHeight \/ 2;\r\n        return Math.sqrt(Math.pow(rect.x - centerX, 2) + Math.pow(rect.y - centerY, 2)) \/ 100;\r\n    }\r\n\r\n    window.addEventListener(\"mousemove\", updateHotspotSize);\r\n});\r\n<\/script>\r\n","custom_css":".lapentor-hotspot img {\r\n    transition: transform 0.3s ease-in-out;\r\n    transform-origin: center center;\r\n}\r\n"}}],"user_id":"66808e19b2b679791706c3f9","public":true,"in_portfolio":false,"updated_at":"2025-04-14 03:13:48","created_at":"2025-01-16 01:07:52","google":[],"meta":{"title":"Condominio los Lobos Pichilemu","image":"\/uploads\/vr\/6787f978658a54d7c201833d\/pano\/vtour\/6787f9883db7ae78650d526c\/thumb.jpg"},"settings":{"auto_rotate":{"active":false,"idle_time":0,"speed":0},"auto_change_scene":{"active":false,"idle_time":0,"random":0}},"downloadable":1,"project_size":0}}],"groups":[]}